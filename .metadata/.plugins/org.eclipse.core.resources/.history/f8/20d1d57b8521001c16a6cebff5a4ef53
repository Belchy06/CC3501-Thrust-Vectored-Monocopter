#ifndef SOURCES_CONTROLLERS_H_
#define SOURCES_CONTROLLERS_H_

typedef struct Estimator {
	float x[4];
	float P[16];

	void (*estimate)(struct Estimator *self, const float measurements[4], const float delta_t);
	void (*getEstimate)(struct Estimator *self, float *xEst, float *yEst);
} VelocityEstimator;



#endif /* SOURCES_CONTROLLERS_H_ */
